    "@util": "<rootDir>/src/util",
    "@types": "<rootDir>/src/types",
    "@redux": "<rootDir>/src/redux",
    "@protons": "<rootDir>/src/components/0-protons",
    "@atoms": "<rootDir>/src/components/1-atoms",
    "@molecules": "<rootDir>/src/components/2-molecules",
    "@organisms": "<rootDir>/src/components/3-organisms",
    "@templates": "<rootDir>/src/components/templates",
    "@pages": "<rootDir>/src/components/pages"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^9.0.2",
    "tailwindcss": "^2.2.19"
  }
